{
    "name": "tonylabs/canvas-lms-api-adapter",
    "description": "PHP adapter for Canvas LMS API integration",
    "keywords": ["Canvas LMS", "API", "Education", "LMS"],
    "version": "1.0.2",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "TONYLABS",
            "email": "support@tonylabs.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "TONYLABS\\Canvas\\": "src"
        }
    },
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/console": ">=9.0",
        "illuminate/support": ">=9.0"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
